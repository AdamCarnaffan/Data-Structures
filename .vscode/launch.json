{
   "version": "0.2.0",
   "configurations": [
      {
         "name": "C: Debug Current File",
         "type": "cppdbg",
         "request": "launch",
         "program": "${workspaceFolder}/a.exe",
         "args": [],
         "stopAtEntry": false,
         "cwd": "${workspaceFolder}",
         "environment": [],
         "externalConsole": false,
         "MIMode": "gdb",
         "miDebuggerPath": "F:\\gcc\\bin\\gdb.exe",
         "internalConsoleOptions": "openOnSessionStart",
         "setupCommands": [
            {
               "description": "Enable pretty-printing for gdb",
               "text": "-enable-pretty-printing",
               "ignoreFailures": true
            }
         ],
         "preLaunchTask": "Build C File"
      },
      {
         "name": "Python: Current File (Integrated Terminal)",
         "type": "python",
         "request": "launch",
         "program": "${file}",
         "env": {"PYTHONPATH": "${fileDirname}"},
         "cwd": "${fileDirname}",
         "console": "integratedTerminal"
      },
      {
         "name": "Python: Test Project",
         "type": "python",
         "request": "launch",
         "program": "${fileDirname}/test.py",
         "env": {"PYTHONPATH": "${fileDirname}"},
         "cwd": "${fileDirname}",
         "console": "integratedTerminal"
      },
      {
         "name": "Python: Attach",
         "type": "python",
         "request": "attach",
         "port": 5678,
         "host": "localhost"
      },
      {
         "name": "Python: Module",
         "type": "python",
         "request": "launch",
         "module": "enter-your-module-name-here",
         "console": "integratedTerminal"
      },
      {
         "name": "Python: Django",
         "type": "python",
         "request": "launch",
         "program": "${workspaceFolder}/manage.py",
         "console": "integratedTerminal",
         "args": [
            "runserver",
            "--noreload",
            "--nothreading"
         ],
         "django": true
      },
      {
         "name": "Python: Flask",
         "type": "python",
         "request": "launch",
         "module": "flask",
         "env": {
            "FLASK_APP": "app.py"
         },
         "args": [
            "run",
            "--no-debugger",
            "--no-reload"
         ],
         "jinja": true
      },
      {
         "name": "Python: Current File (External Terminal)",
         "type": "python",
         "request": "launch",
         "program": "${file}",
         "console": "externalTerminal"
      }
   ]
}